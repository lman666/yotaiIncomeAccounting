<view class="body"><view class="topic"><text>发电量测算报告</text></view><view class="provincialInfo"><text class="title">{{calculationResults[0].city}}</text><view class="info"><block wx:for="{{calculationResults}}" wx:for-item="item" wx:for-index="i" wx:key="i"><block><block wx:if="{{i>=1&&i<=7&&i!==5}}"><yotai-row-cell vue-id="{{'02403c40-1-'+i}}" label="{{item.label}}" num="{{item.num}}" unit="{{item.unit}}" bind:__l="__l"></yotai-row-cell></block><block wx:if="{{i===5}}"><yotai-row-cell vue-id="{{'02403c40-2-'+i}}" label="{{item.label}}" num="{{item.num}}" unit="{{item.unit}}" isTotalSystemPower="{{true}}" bind:__l="__l"></yotai-row-cell></block></block></block></view></view><view class="chargeAndDischargeCapacity"><text class="title">充放电量</text><view class="info"><block wx:for="{{calculationResults}}" wx:for-item="item" wx:for-index="i" wx:key="i"><block><block wx:if="{{i>=8&&i<=9}}"><yotai-row-cell vue-id="{{'02403c40-3-'+i}}" label="{{item.label}}" num="{{item.num}}" unit="{{item.unit}}" bind:__l="__l"></yotai-row-cell></block></block></block></view></view><view class="income"><text class="title">收益</text><view class="info"><block wx:for="{{calculationResults}}" wx:for-item="item" wx:for-index="i" wx:key="i"><block><block wx:if="{{i>=10&&i<=14&&item.num!==undefined}}"><yotai-row-cell vue-id="{{'02403c40-4-'+i}}" label="{{item.label}}" num="{{item.num}}" unit="{{item.unit}}" bind:__l="__l"></yotai-row-cell></block></block></block></view></view><view data-event-opts="{{[['tap',[['depthMeasurement',['$event']]]]]}}" class="depthMeasurement" bindtap="__e"><navigator url="navigate/navigate?title=navigate" hover-class="navigator-hover"><text>深度测算>></text></navigator></view><yotai-footer vue-id="02403c40-5" bind:__l="__l"></yotai-footer></view>